import random

# Cadastrar os participantes
participantes = []

print("Digite os nomes dos participantes (digite 'sair' para finalizar):")
while True:
    nome = input("Nome: ")
    if nome.lower() == "sair":
        break
    participantes.append(nome)

# Escolher um ganhador aleatório
vencedor = random.choice(participantes)

# Exibir o nome do vencedor
print(f"\nO vencedor do sorteio é: {vencedor}")